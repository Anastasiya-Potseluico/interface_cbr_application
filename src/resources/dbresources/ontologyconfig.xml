        <DescriptionMappings>
                <Map>
                        <Property>HAS-VIDEO-CAMERA</Property>
                        <Concept>USER_VIDEO_CAMERA</Concept>
                        <Attribute>USER_VIDEO_CAMERA</Attribute>
                </Map>
                <Map>
                        <Property>HAS-AUDIO-COMPONENT</Property>
                        <Concept>USER_AUDIO_COMPONENT</Concept>
                        <Attribute>USER_AUDIO_COMPONENT</Attribute>
                </Map>
                <Map>
                        <Property>HAS-MICROPHONE</Property>
                        <Concept>USER_MICROPHONE</Concept>
                        <Attribute>USER_MICROPHONE</Attribute>
                </Map>
                <Map>
                        <Property>HAS-SPECIAL-DEVICE</Property>
                        <Concept>USER_SPECIAL_DEVICE</Concept>
                        <Attribute>USER_SPECIAL_DEVICE</Attribute>
                </Map>
                <Map>
                        <Property>HAS-MAIN-DEVICE</Property>
                        <Concept>USER_MAIN_DEVICE</Concept>
                        <Attribute>USER_MAIN_DEVICE</Attribute>
                </Map>
                <Map>
                        <Property>HAS-DISEASE-RANGE</Property>
                        <Concept>USER_DISEASE_RANGE</Concept>
                        <Attribute>USER_DISEASE_RANGE</Attribute>
                </Map>
                <Map>
                        <Property>HAS-DISEASE</Property>
                        <Concept>USER_DISEASE</Concept>
                        <Attribute>USER_DISEASE</Attribute>
                </Map>
                <Map>
                        <Property>HAS-AGE</Property>
                        <Concept>USER_AGE</Concept>
                        <Attribute>USER_AGE</Attribute>
