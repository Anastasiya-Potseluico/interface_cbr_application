                        <Attribute>USER_INPUT_DATA_COMPONENT</Attribute>
                </Map>
				<Map>
                        <Property>HAS-OUTPUT-DATA-COMPONENT</Property>
                        <Concept>USER_OUTPUT_DATA_COMPONENT</Concept>
